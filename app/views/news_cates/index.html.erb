<h1>Listing news_cates</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @news_cates.each do |news_cate| %>
  <tr>
    <td><%= news_cate.name %></td>
    <td><%= link_to 'Show', news_cate %></td>
    <td><%= link_to 'Edit', edit_news_cate_path(news_cate) %></td>
