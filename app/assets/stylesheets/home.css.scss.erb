@charset "utf-8";
/* CSS Document */
body{ margin:0; padding:0;background:#BFDBE3;}
ul,li,p{ margin:0; padding:0;}
img { border:none;}
li{ list-style:none;}
#wrapper{ background:url(<%= asset_path 'ffff.png' %>) no-repeat;}
.head{ width:936px; margin:0 auto;}
.logo{ padding:10px 4px;}
.menu{ background:url(<%= asset_path 'indexbg.gif' %>) repeat-x 0 0; height:48px; line-height:48px; margin-top:8px;}
.meuu2{ width:936px; margin:0 auto;}
.meuu2 li{ float:left; margin:0 12px; font-size:20px; color:#fff; font-family:"微软雅黑"; position:relative;}
.meuu2 li a{ text-decoration:none; color:#fff;}
.current{ background:url(<%= asset_path 'indexbg.gif' %>) no-repeat 0 -65px; width:123px; height:50px; text-align:center; position:absolute; top:-7px; padding-top:8px;}
.banner{ margin-top:10px; width:936px; margin:0 auto;}
.tuigi{ width:956px; margin:0 0 0 -10px; height:114px;}
.content{ width:936px!important;_width:910px; margin:0 auto; background:#fff; }
.msruby,.msruby2{ padding-top:15px; font-size:12px; overflow:hidden; width:936px;}
.msruby li{ float:left; margin-left:23px!important;*margin-left:13px; border:1px solid #e9e9e9; width:430px; margin-bottom:25px; overflow:hidden; padding:8px 2px;}
.msruby2 li{ float:left; margin-left:23px!important;*margin-left:13px; width:430px; margin-bottom:25px; overflow:hidden; padding:8px 2px;}
.icon{ background:url(<%= asset_path 'indexbg.gif' %>) no-repeat -6px -123px; width:103px; height:100px; }
.icon2{ background:url(<%= asset_path 'indexbg.gif' %>) no-repeat -123px -123px; width:111px; height:100px;}
.icon3{ background:url(<%= asset_path 'indexbg.gif' %>) no-repeat -259px -132px; width:111px; height:100px;}
.icon4{ background:url(<%= asset_path 'indexbg.gif' %>) no-repeat -390px -132px; width:92px; height:86px;}
.icon,.icon2,.icon3,.icon4{float:left;}
.ruby-wz{ margin-left:15px; width:260px; line-height:20px; float:left;}
.cotnet2{ margin:0px 26px; clear:both; font-size:12px; overflow:hidden;}
.cotnet3{ margin:0px 26px; clear:both; font-size:12px; overflow:hidden;}
.tel{ line-height:24px; font-size:14px; margin-bottom:15px; color:#585858;}
.cotnet2 h3{ color:#267CB0; border-bottom:1px solid #B0B0B0; padding-bottom:4px; font-family:"微软雅黑"; font-size:18px;}
.cotnet3 h3{ color:#267CB0; border-bottom:1px solid #B0B0B0; padding-bottom:4px; font-family:"微软雅黑"; font-size:18px;}
.cotnet2 li{ float:left; margin:8px 8px; border:1px solid #e9e9e9; padding:4px;}
.cotnet2 li a{ color:#333; text-decoration:none;}
.about{ border-bottom:1px solid #C78B97; margin:14px 24px; font-size:12px; color:#6A6A6A; padding-bottom:4px;}
.about strong{ font-size:16px;}
.jsruby{ margin-top:15px; margin-bottom:10px;margin-left:26px; overflow:hidden; font-size:12px; color:#4F4F4F; line-height:20px;}
.jsruby li{ float:left; width:403px; margin-right:34px; padding-bottom:10px;}
.title{ margin-bottom:20px; font-weight:bold; color:#3589A4;}

.rupic,.rupic1,.rupic2,.rupic3{ float:left; margin-left:10px;}
.rupic{ background:url(<%= asset_path 'aboutus.png' %>) no-repeat 0 0; width:60px; height:62px;}
.rupic1{ background:url(<%= asset_path 'aboutus.png' %>) no-repeat -77px 0; width:58px; height:65px;}
.rupic2{ background:url(<%= asset_path 'aboutus.png' %>) no-repeat -158px 0; width:53px; height:62px;}
.rupic3{ background:url(<%= asset_path 'aboutus.png' %>) no-repeat -219px 0; width:63px; height:65px;}



/* 关于我们 */
.about1{ float:left; width:550px; font-size:14px; line-height:24px; margin-bottom:15px;}
.right{ float:left; border-left:1px solid #e9e9e9; height:auto; margin-left:30px;}
.right h3{ border-bottom:1px solid #e9e9e9; margin:0; padding:0; width:290px; padding-bottom:5px; padding-left:10px; font-size:16px; font-weight:normal; color:#333; overflow:hidden;}
.right h3 a{ font-size:12px; text-decoration:none; color:#333;}
.aboutrub{ line-height:26px; margin:10px 0 15px;}
.aboutrub li{ border-bottom:1px  dotted #e9e9e9; padding-left:6px;}
.aboutrub li a{ text-decoration:none; color:#333;}
.aboutrub li a:hover{ text-decoration:underline; color:#333;}



/* 案例展示 */
.case_list_link{padding: 10px;}
.case_list_link img{width: 300px;}

.titleabt{ text-align:center; font-size:16px; font-weight:bold;}
.rublist{ overflow:hidden; margin:15px 10px 15px 13px;color:#3B3B3B;}
label{ float:left; width:80px; text-align:right; }
.texbtn{float:left; margin-left:8px; height:27px; border:1px solid #CACACA; line-height:27px; width:230px;}
.texbtn2{float:left;margin-left:8px;width:330px; height:80px;}


/* 业界动态 */
.left{ float:left; width:160px; padding-left:15px;}
.list{ float:left; margin-left:10px; width:650px; margin-bottom:15px; line-height:22px; font-size:12px; border-left:1px solid #C7C7C7; padding-left:6px;}
.news_main{ float:left; margin-left:10px; width:740px!important; width: 700px; margin-bottom:15px; line-height:22px; font-size:12px; border-left:1px solid #C7C7C7; padding-left:6px;}
.left h3{ background:url(<%= asset_path 'titlebg.jpg' %>) repeat-x; padding:0; margin:0; height:35px; line-height:35px; padding-left:4px; color:#454545;}
.left li{ border-bottom:1px solid #E0E0E0; line-height:30px; }
.left li a{ text-decoration:none; color:#333; font-weight:bold;}
.no1{background:url(<%= asset_path 'iconav.png' %>) no-repeat 0px 6px;  padding-left:28px;display: block;height: 36px;}
.no2{background:url(<%= asset_path 'iconav.png' %>) no-repeat 0 -213px; padding-left:28px;display: block;height: 36px;}
.no3{background:url(<%= asset_path 'iconav.png' %>) no-repeat 0 -141px; padding-left:28px;display: block;height: 36px;}
.no4{background:url(<%= asset_path 'iconav.png' %>) no-repeat 0 6px;    padding-left:28px;display: block;height: 36px;}
.no5{background:url(<%= asset_path 'iconav.png' %>) no-repeat 0 -213px; padding-left:28px;display: block;height: 36px;}
.no6{background:url(<%= asset_path 'iconav.png' %>) no-repeat 0 -141px; padding-left:28px;display: block;height: 36px;}
.no7{background:url(<%= asset_path 'iconav.png' %>) no-repeat 0 -213px; padding-left:28px;display: block;height: 36px;}
.no8{background:url(<%= asset_path 'iconav.png' %>) no-repeat 0 -141px; padding-left:28px;display: block;height: 36px;}

.titlbg{ background:url(<%= asset_path 'titlebgg.png' %>); width:97px; padding:0; margin:0; height:35px; line-height:35px; color:#FFF; text-align:center;}
.list li{ border-bottom:1px solid #ccc; margin-bottom:10px; padding-bottom:8px;}
.ztn a{ text-decoration:none; color:#333;}
.ztn a:hover{ text-decoration:underline;}
.titbg{ margin:0; padding:0; }
.titbg a{ text-decoration:none;color:#168CB2;}
.titbg a:hover{ text-decoration:underline;}
.titlerb{text-align:center; border-bottom:1px dashed #e9e9e9; padding-bottom:10px; margin-bottom:15px; }
.titlerb h1{font-size:28px; font-weight:bold; color:#4C4B4B; margin: 0; padding: 0; line-height: 42px; font-family: "微软雅黑"}
.news_content{font-size: 14px; line-height: 28px; padding: 10px; font-family: "微软雅黑" }

.footer{ width:918px; margin:0 auto; background:#fff; font-size:12px; margin-top:15px; padding:10px 5px 15px 13px; color:#595959;}
.footer a{ text-decoration:none; color:#3589A4;}
.footer a:hover{ text-decoration:underline;}